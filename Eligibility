C++ program to check age of person eligible to vote 

#include<iostream>
using namespace std;
int main()
{
     int age ;
     cout <<"Enter age of the person: ";
     cin >> age;
     
     if (age>=18)
     {
     	cout << "\n Person is eligible to vote ";
    
	 }
	 else
	 {
	 	cout << "Person is not eligible to vote"<<"\n";
    cout << "Wait for the person to turn 18";
	
	 }
	return 0;
}
